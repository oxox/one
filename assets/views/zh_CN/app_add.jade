<div class="config_pop open">
	<form class="form-horizontal">
		<fieldset>
			<div class="control-group">
				<label class="control-label" for="appLink">应用地址：</label>
				<div class="controls">
					<input type="text" name="" id="appLink">
				</div>
			</div>
			<div class="control-group">
				<label class="control-label" for="appName">应用标题：</label>
				<div class="controls">
					<input type="text" name="" id="appName">
				</div>
			</div>
			<div class="control-group">
				<label class="control-label" for="appDisc">应用描述：</label>
				<div class="controls">
					<textarea name="" id="appDisc" cols="30" rows="10"></textarea>
				</div>
			</div>
			<div class="form-actions">
				<button type="submit" class="btn btn-primary">完成</button>
				<button class="btn">取消</button>
			</div>
		</fieldset>
	</form>
</div>